<doctrine-mapping xmlns="https://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="https://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="Calendar\Infrastructure\Calendar\Calendar" table="calendars">
        <id name="id" type="string" column="id"/>
        <field name="title" type="string" column="title"/>
        <field name="isPublic" type="boolean" column="is_public"/>
        <field name="ownerId" type="string" column="owner_id"/>
        <field name="dates" type="string" column="dates"/>
        <field name="createdAt" type="datetime" column="created_at"/>
    </entity>

</doctrine-mapping>